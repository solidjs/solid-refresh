{
  "name": "solid-refresh",
  "description": "Universal HMR for SolidJS",
  "author": "Ryan Carniato",
  "license": "MIT",
  "version": "0.4.1",
  "homepage": "https://github.com/solidjs/solid-refresh#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/solidjs/solid-refresh"
  },
  "main": "dist/solid-refresh.js",
