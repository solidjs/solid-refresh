    "@types/babel__generator": "^7.6.4",
    "@types/node": "^18.13.0",
    "prettier": "^2.8.4",
    "rollup": "^3.15.0",
    "solid-js": "^1.6.10",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "vitest": "^0.28.4"
  },
  "peerDependencies": {
    "solid-js": "^1.3"
  },
  "dependencies": {
    "@babel/generator": "^7.20.14",
